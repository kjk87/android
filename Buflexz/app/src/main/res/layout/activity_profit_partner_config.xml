<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar_page"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_1b1b1b"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:elevation="@dimen/height_0">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:id="@+id/collapsing_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_1b1b1b"
            app:contentScrim="@color/color_1b1b1b"
            app:expandedTitleTextAppearance="@style/TransparentText"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/width_44"
                    android:layout_marginTop="@dimen/height_22"
                    android:layout_marginEnd="@dimen/width_44"
                    android:background="@drawable/bg_232323_radius_55"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/width_50"
                    android:paddingTop="@dimen/height_66"
                    android:paddingEnd="@dimen/width_50"
                    android:paddingBottom="@dimen/height_66">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/word_total_commission"
                        android:textColor="@color/color_77f5ae"
                        android:textSize="@dimen/textSize_44pt"
                        android:textStyle="bold" />

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/width_8"
                            android:text="@string/word_dollar_unit"
                            android:textColor="@color/color_dcdcdc"
                            android:textSize="@dimen/textSize_38pt" />

                        <TextView
                            android:id="@+id/text_profit_partner_config_total_price"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/white"
                            android:textSize="@dimen/textSize_44pt"
                            android:textStyle="bold" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_38pt"
                    android:layout_marginTop="@dimen/height_45"
                    android:layout_marginStart="@dimen/width_44"
                    android:layout_marginEnd="@dimen/width_44"
                    android:text="@string/msg_profit_partner_desc"
                    android:layout_marginBottom="@dimen/height_116"
                    android:textColor="@color/white"/>


            </androidx.appcompat.widget.LinearLayoutCompat>

        </com.google.android.material.appbar.CollapsingToolbarLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/height_33"
            android:paddingStart="@dimen/width_44"
            android:paddingEnd="@dimen/width_44">

            <ImageView
                android:id="@+id/image_profit_partner_month_before"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_month_before"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_44"
                android:layout_toStartOf="@id/text_profit_partner_month"/>

            <TextView
                android:id="@+id/text_profit_partner_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize_44pt"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/image_profit_partner_month_next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_month_next"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_44"
                android:layout_toEndOf="@id/text_profit_partner_month"/>

        </RelativeLayout>


    </com.google.android.material.appbar.AppBarLayout>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/layout_profit_partner_config_exist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="@dimen/width_44"
            android:paddingEnd="@dimen/width_44"
            android:paddingTop="@dimen/height_38"
            android:paddingBottom="@dimen/height_38"
            android:background="@color/color_232323">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/height_33">

                <TextView
                    android:id="@+id/text_profit_partner_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_33pt"
                    android:textColor="@color/color_b0b0b0"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:id="@+id/text_profit_partner_status"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_66"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:background="@drawable/bg_dbb5b5b5_radius_20"
                    android:paddingStart="@dimen/width_7"
                    android:paddingEnd="@dimen/width_7"
                    android:textSize="@dimen/textSize_33pt"
                    android:textColor="@color/color_3e78ff"
                    android:gravity="center"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_33pt"
                    android:textColor="@color/color_dcdcdc"
                    android:text="@string/word_my_profit"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:id="@+id/text_profit_partner_config_my_profit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_38pt"
                    android:textColor="@color/color_dcdcdc"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_profit_partner_config_transfered_profit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/height_16">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_33pt"
                    android:textColor="@color/color_dcdcdc"
                    android:text="@string/word_transfered_profit"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:id="@+id/text_profit_partner_config_transfered_profit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_38pt"
                    android:textColor="@color/color_dcdcdc"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_profit_partner_config_master_profit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/height_16">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_33pt"
                    android:textColor="@color/color_dcdcdc"
                    android:text="@string/word_master_profit"
                    android:gravity="center"
                    android:drawablePadding="@dimen/width_10"
                    android:layout_centerVertical="true"
                    app:drawableStartCompat="@drawable/ic_master_profit_question" />

                <TextView
                    android:id="@+id/text_profit_partner_config_master_profit_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_66"
                    android:textColorHint="@color/color_dcdcdc"
                    android:paddingStart="@dimen/width_11"
                    android:paddingEnd="@dimen/width_11"
                    android:gravity="center"
                    android:layout_toStartOf="@id/text_profit_partner_config_master_profit"
                    android:text="@string/word_detail_history"
                    android:layout_marginEnd="@dimen/width_11"
                    android:layout_centerVertical="true"
                    android:background="@drawable/bg_d6000000_radius_20"/>

                <TextView
                    android:id="@+id/text_profit_partner_config_master_profit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_38pt"
                    android:textColor="@color/color_dcdcdc"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/height_22">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_38pt"
                    android:textColor="@color/color_dcdcdc"
                    android:text="@string/word_total_profit"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:id="@+id/text_profit_partner_config_total_profit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_44pt"
                    android:textColor="@color/color_dcdcdc"
                    android:textStyle="bold"
                    android:textColorHint="@color/white"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"/>

            </RelativeLayout>


        </androidx.appcompat.widget.LinearLayoutCompat>


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/layout_profit_partner_config_not_exist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:visibility="gone"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/height_500"
            android:text="@string/msg_not_exist_profit_partner"
            android:textSize="@dimen/textSize_40pt"/>


    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.coordinatorlayout.widget.CoordinatorLayout>


