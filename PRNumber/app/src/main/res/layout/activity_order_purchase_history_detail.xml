                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_3"
                    android:layout_marginStart="@dimen/width_60"
                    android:layout_marginTop="@dimen/height_60"
                    android:background="@color/color_eef2f6" />

                <RelativeLayout
                    android:id="@+id/layout_order_purchase_history_detail_start_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/width_60"
                    android:layout_marginTop="@dimen/height_36"
                    android:layout_marginEnd="@dimen/width_60"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/word_delivery_start_time"
                        android:textColor="@color/color_8c969f"
                        android:textSize="@dimen/textSize_42pt" />

                    <TextView
                        android:id="@+id/text_order_purchase_history_detail_delivery_start_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/color_373c42"
                        android:textSize="@dimen/textSize_42pt"
                        android:textStyle="bold" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/layout_order_purchase_history_detail_complete_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/width_60"
                    android:layout_marginTop="@dimen/height_36"
                    android:layout_marginEnd="@dimen/width_60"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/text_order_purchase_history_detail_complete_time_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/word_visit_complete_time"
                        android:textColor="@color/color_8c969f"
                        android:textSize="@dimen/textSize_42pt" />

                    <TextView
                        android:id="@+id/text_order_purchase_history_detail_complete_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/color_373c42"
                        android:textSize="@dimen/textSize_42pt"
                        android:textStyle="bold" />

                </RelativeLayout>
            </androidx.appcompat.widget.LinearLayoutCompat>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_24"
                android:layout_marginTop="@dimen/height_60"
                android:background="@color/color_f7fafc" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_60"
                android:layout_marginTop="@dimen/height_60"
                android:text="@string/word_order_history"
                android:textColor="@color/color_373c42"
                android:textSize="@dimen/textSize_54pt"
                android:textStyle="bold" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recycler_order_purchase_history_detail_menu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_marginStart="@dimen/width_60"
                android:layout_marginTop="@dimen/height_60"
                android:background="@color/color_eef2f6" />

            <RelativeLayout
                android:id="@+id/layout_order_purchase_history_detail_delivery_fee"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/width_60"
                android:layout_marginTop="@dimen/height_60"
                android:layout_marginEnd="@dimen/width_60">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/word_delivery_price"
                    android:textColor="@color/color_373c42"
                    android:textSize="@dimen/textSize_42pt" />

                <TextView
                    android:id="@+id/text_order_purchase_history_detail_delivery_fee"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/color_373c42"
                    android:textSize="@dimen/textSize_42pt"
                    android:textStyle="bold" />

            </RelativeLayout>

            <View
