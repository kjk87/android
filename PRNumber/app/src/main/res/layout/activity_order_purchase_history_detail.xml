            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_order_purchase_history_detail_save_point"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_180"
                android:gravity="center_vertical"
                android:layout_marginStart="@dimen/width_60"
                android:layout_marginEnd="@dimen/width_60"
                android:layout_marginTop="@dimen/height_60"
                android:layout_marginBottom="@dimen/height_120"
                android:background="@drawable/bg_f7fafc_radius_12">


                <TextView
                    android:id="@+id/text_order_purchase_history_detail_save_point"
                    android:layout_width="wrap_content"
