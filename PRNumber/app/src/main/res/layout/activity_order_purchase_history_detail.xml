                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/word_cancel_reason"
                        android:textColor="@color/color_8c969f"
                        android:textSize="@dimen/textSize_42pt" />

                    <TextView
                        android:id="@+id/text_order_purchase_history_detail_cancel_memo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:gravity="end"
                        android:textColor="@color/color_373c42"
                        android:textSize="@dimen/textSize_42pt"
                        android:textStyle="bold" />

                </RelativeLayout>

            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.LinearLayoutCompat
