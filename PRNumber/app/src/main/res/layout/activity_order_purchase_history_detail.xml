                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/color_373c42"
                        android:textSize="@dimen/textSize_42pt"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
