<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/layout_product_ship_detail_ticket_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_67"
            android:layout_marginStart="@dimen/width_83"
            android:layout_marginEnd="@dimen/width_83"
            android:layout_marginBottom="@dimen/height_50"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical">

                <ImageView
                    android:layout_width="@dimen/width_33"
                    android:layout_height="@dimen/width_33"
                    android:background="@drawable/bg_000000"
                    android:layout_marginEnd="@dimen/width_17"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_42pt"
                    android:textColor="@color/black"
                    android:text="@string/word_sale_limit_time"
                    android:textStyle="bold"/>

            </LinearLayout>

            <TextView
                android:id="@+id/text_product_ship_detail_sale_limit_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_42pt"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/width_50"
                android:layout_marginTop="@dimen/height_19"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/height_32"
                android:gravity="center_vertical">

                <ImageView
                    android:layout_width="@dimen/width_33"
                    android:layout_height="@dimen/width_33"
                    android:background="@drawable/bg_000000"
                    android:layout_marginEnd="@dimen/width_17"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_42pt"
                    android:textColor="@color/black"
                    android:text="@string/word_use_enable_time"
                    android:textStyle="bold"/>

            </LinearLayout>

            <TextView
                android:id="@+id/text_product_ship_detail_end_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_42pt"
                android:textColor="@color/color_ff0000"
                android:layout_marginStart="@dimen/width_50"
                android:layout_marginTop="@dimen/height_19"
                android:textStyle="bold"/>



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/height_32"
                android:gravity="center_vertical">

                <ImageView
                    android:layout_width="@dimen/width_33"
                    android:layout_height="@dimen/width_33"
                    android:background="@drawable/bg_000000"
                    android:layout_marginEnd="@dimen/width_17"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_42pt"
                    android:textColor="@color/black"
                    android:text="@string/word_origin"
                    android:textStyle="bold"/>

            </LinearLayout>

            <TextView
                android:id="@+id/text_product_ship_detail_origin_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_42pt"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/width_50"
                android:layout_marginTop="@dimen/height_19"/>

        </LinearLayout>

        <com.pplus.prnumberuser.apps.common.component.TouchyWebView
            android:id="@+id/webview_product_ship_detail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_open_webview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_165"
            android:layout_marginStart="@dimen/width_54"
            android:layout_marginTop="@dimen/height_30"
            android:layout_marginEnd="@dimen/width_54"
            android:background="@drawable/border_color_d8d8d8_2px"
            android:visibility="gone">

            <TextView
                android:id="@+id/text_product_ship_detail_open_webview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:drawableEnd="@drawable/ic_arrow_down"
                android:drawablePadding="@dimen/width_20"
                android:gravity="center"
                android:text="@string/msg_open_view_product_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30"
            android:layout_marginTop="@dimen/height_30"
            android:background="@color/color_f8f8f8" />

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_purchase_add_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_purchase_add_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_purchase_origin_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_origin_place_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_notice_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132">

            <TextView
                android:id="@+id/text_product_ship_detail_notice_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_goods_notice_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_refund_exchange_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_refund_exchange_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_seller_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_seller_info"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_product_ship_detail_warning"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_132">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:text="@string/word_warning"
                android:textColor="@color/color_232323"
                android:textSize="@dimen/textSize_45pt" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/width_54"
                android:src="@drawable/ic_arrow_black" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_3"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/width_54"
                android:background="@color/color_f8f8f8" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30"
            android:background="@color/color_f8f8f8" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_f8f8f8">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/width_54"
                android:layout_marginEnd="@dimen/width_54"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/height_30"
                    android:layout_marginBottom="@dimen/height_30"
                    android:text="@string/msg_legal_notice_desc"
                    android:textColor="@color/color_808080"
                    android:textSize="@dimen/textSize_26pt" />

            </LinearLayout>

        </RelativeLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_30"
            android:layout_marginTop="@dimen/height_30"
            android:background="@color/color_f8f8f8" />


    </LinearLayout>

</LinearLayout>